{
    "name": "hounddog/doctrine-data-fixture-module",
    "description": "Zend Framework 2 Module that provides Doctrine Data-Fixture functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf2"
    ],
    "homepage": "http://www.doctrine-project.org/",
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-modulemanager": "^2.0",
        "zendframework/zend-eventmanager": "^2.0",
        "zendframework/zend-servicemanager": "^2.0 || ^3.0",
        "doctrine/data-fixtures": "^1.1",
        "doctrine/doctrine-orm-module": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^3.7",
        "satooshi/php-coveralls": "^0.6",
        "psr/log": "^1.0",
        "squizlabs/php_codesniffer": "^1.4"   
    },
    "autoload": {
        "psr-0": {
            "DoctrineDataFixtureModule": "src/"
        }
    }
}
