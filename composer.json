{
  "name" : "samuel20miglia/doctrine-data-fixture-module",
  "description" : "Zend Framework 2 Module that provides Doctrine Data-Fixture functionality",
  "require" : {
    "php" : ">=5.6",
    "zendframework/zend-modulemanager" : "2.*",
    "zendframework/zend-eventmanager" : "2.*",
    "zendframework/zend-servicemanager" : "2.*",
    "doctrine/data-fixtures" : "v1.2.2",
    "doctrine/doctrine-orm-module" : "~1.1.1"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*",
    "satooshi/php-coveralls" : ">=0.6.0",
    "squizlabs/php_codesniffer" : "1.4.*"
  },
  "license" : "MIT",
  "keywords" : [ "doctrine", "data-fixture", "module", "zf2" ],
  "autoload" : {
    "psr-0" : {
      "DoctrineDataFixtureModule" : "src/"
    }
  },
  "type" : "library",
  "homepage" : "http://www.doctrine-project.org/",
  "authors" : [ {
    "name" : "Martin Shwalbe",
    "email" : "martin.shwalbe@gmail.com"
  },
  {
    "name" : "Samuel Ventimiglia",
    "email" : "sam.ventimiglia@gmail.com"
  } ]
}